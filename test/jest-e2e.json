{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/alt-testlib/(.*)": "<rootDir>/../libs/alt-testlib/src/$1",
    "@app/alt-testlib": "<rootDir>/../libs/alt-testlib/src",
    "alt/alt-auth/(.*)": "<rootDir>/../libs/alt-auth/src/$1",
    "alt/alt-auth": "<rootDir>/../libs/alt-auth/src",
    "@alt/alt-user/(.*)": "<rootDir>/../libs/alt-user/src/$1",
    "@alt/alt-user": "<rootDir>/../libs/alt-user/src"
  }
}